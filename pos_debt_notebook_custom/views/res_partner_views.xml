<odoo>
    <record id="view_partner_form_debt" model="ir.ui.view">
        <field name="name">res.partner.form.debt</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="point_of_sale.view_partner_property_form"/>
